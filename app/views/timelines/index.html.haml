= content_for(:title) do
  = "#{@user.first_name} #{@user.last_name} - #{@user.description} | Timeliner"
= content_for(:description) do
  = @user.bio

.wrapper
  = render 'header'

  = render 'about'

  #skills.container
    .row
      .col-100
        - @user.skills.each do |skill|
          %span= skill.name

  = render 'timeline'
  = render 'projects', user: @user
  = render 'contact', user: @user
  = render 'social_media'
  = render 'footer'
= render 'overlay'
